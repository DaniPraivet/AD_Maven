<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dev.danipraivet.app.Autores" table="Autores">
        <meta attribute="class-description">
            This class contains authors of the library
        </meta>
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="nombre" column="nombre" type="string"/>
        <property name="apellido" column="apellido" type="string"/>
        <property name="nacionalidad" column="nacionalidad" type="string"/>
    </class>
</hibernate-mapping>